{
    "name": "razorpay/auth-service",
    "description": "Razorpay's Auth service",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "rmccue/requests": "^2.0.4",
        "vlucas/phpdotenv": "^5.4.1",
        "razorpay/trace": "^4.0.0",
        "laravel/lumen-framework": "^9.1.0",
        "razorpay/spine": "^2.0.0",
        "lcobucci/jwt":"4.1.5",
        "razorpay/oauth":"dev-oauth_dualwrite",
        "razorpay/outbox-php": "^2.0.0",
        "laravel/helpers": "^1.5",
        "ext-json": "*",
        "opencensus/opencensus": "~0.7",
        "opencensus/opencensus-exporter-jaeger": "^v0.1.2-alpha",
        "laravel/legacy-factories": "^1.3",
        "predis/predis": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/spine.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/oauth.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/trace.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/metrics-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/outbox-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/opencensus-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/opencensus-php-exporter-jaeger.git"
        }
    ],
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "mockery/mockery": "^1.5.0",
        "kint-php/kint": "^4.1.3",
        "phpunit/phpunit": "^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
