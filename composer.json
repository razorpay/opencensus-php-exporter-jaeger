{
    "name": "razorpay/auth-service",
    "description": "Razorpay's Auth service",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/lumen-framework": "^11.0",
        "rmccue/requests": "^2.0.4",
        "vlucas/phpdotenv": "^5.6",
        "razorpay/trace": "dev-illuminate-upgrade",
        "razorpay/oauth": "dev-oauth-upgrade",
        "lcobucci/jwt": "^4.1",
        "laravel/helpers": "^1.7",
        "ext-json": "*",
        "opencensus/opencensus": "^1.0.0|^0.4",
        "opencensus/opencensus-exporter-jaeger": "^v0.2.0",
        "laravel/legacy-factories": "^1.3",
        "predis/predis": "^2.2",
        "grpc/grpc": "^1.57",
        "ext-grpc": "*",
        "google/common-protos": "^4.8",
        "razorpay/outbox-php": "dev-dev-lumen-fix",
        "razorpay/spine": "dev-dev-laravel-update"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/trace.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/metrics-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/outbox-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/spine.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/opencensus-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/opencensus-php-exporter-jaeger.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:razorpay/oauth.git"
        }
    ],
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6.0",
        "kint-php/kint": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
